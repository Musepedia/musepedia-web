<!--custum_tabbar/index.wxml-->
<van-tabbar
  class="custom-tabbar"
  active="{{ active }}"
  active-color="#4834d4"
  inactive-color="#000"
  bind:change="onChange"
>
  <van-tabbar-item safe-area-inset-bottom="false" icon="home-o">首页</van-tabbar-item>
  <van-tabbar-item safe-area-inset-bottom="false" icon="notes-o">发现</van-tabbar-item>
  <van-tabbar-item safe-area-inset-bottom="false" icon="calendar-o">社区</van-tabbar-item>
  <van-tabbar-item safe-area-inset-bottom="false" icon="friends-o">个人</van-tabbar-item>
</van-tabbar>
